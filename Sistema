function dy=Sistema(t, y)
%-----------Definicion de los parametros----------%
m = 10;
r = 0.05;
k = 100;
%--------------------------------------------------%
dy = zeros(2,1);
%-------Definicion de la dinamica del sistema------%
y1 = y(1); % x1
y2 = y(2); % dx1/dt

dy(1) = y2;
dy(2) = (((-2*k)/(3*m))*y1);
%--------------------------------------------------%

